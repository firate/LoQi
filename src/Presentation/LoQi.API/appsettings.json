{
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=/Users/firatergul/Developer/databases/loqi.db;"
  },
  "StaticFiles": {
    "EnableBrotliCompression": true,
    "EnableGzipCompression": true,
    "CacheMaxAge": "30.00:00:00"
  },
  "UdpListener": {
    "Port": 10080,
    "ChannelCapacity": 5000,
    "ProcessingStatsInterval": 60
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "LoQi.Application.Services.UdpPackageListener": "Debug",
      "LoQi.API.BackgroundServices.UdpLogProcessingService": "Debug"
    }
  },
  "LoQi": {
    "Notifications": {
      "Enabled": true,
      "ChannelCapacity": 10000
    },
    "Batching": {
      "BatchSize": 500,
      "FlushIntervalMs": 1000,
      "MaxMemoryMb": 100,
      "MaxBufferSize": 50000
    }
  }
}
